  "version": "1.7.1",
  "productName": "Bcb Wallet ",
  "genericName": "Wallet",
  "description": "An ambitious bcb wallet for desktop and web.",
  "license": "MIT",
  "main": "ember-electron/main.js",
  "author": {
    "name": "Eman Herawy",
    "email": "eman.herawy@gmail.com"
  },
  "homepage": "https://nanowalletcompany.com",
  "copyright": "Copyright Â© 2019 cbc Wallet ",
  "bugs": {
    "url": "https://github.com/bitcoin-black-bcb/bcb-wallet/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitcoin-black-bcb/bcb-wallet"
  },
  "build": {
    "appId": "com.nanowalletcompany.desktop",
    "mac": {
      "category": "public.app-category.finance"
    },
    "linux": {
      "desktop": {
        "Categories": "P2P;Finance;Security"
      }
    }
  },
  "scripts": {
    "build": "ember build",
    "start": "ember serve",
    "test": "ember test",
    "lint:js": "eslint .",
    "lint:hbs": "ember-template-lint .",
    "cz": "git-cz",
    "release": "standard-version",
    "electron:dev": "nodemon -L -w ember-electron -x 'yarn run-s clean electron:start'",
    "electron:start": "ember electron",
    "clean": "run-p clean:*",
    "clean:tmp": "rimraf tmp",
    "clean:dist": "rimraf dist",
    "pack": "build --dir",
    "dist": "build",
    "clean:electron-out": "rimraf electron-out",
    "build-linux": "electron-packager . bcb-app --platform linux --overwrite --asar --arch x64 --out dist/",
    "build-win": "electron-packager . bcb-app --overwrite --asar --platform=win32 --arch x64 --out dist/",
    "deb64": "electron-installer-debian --src dist/bcb-app-linux-x64/ --dest dist/installers/ --arch amd64",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --asar  --icon=ember-electron/resources/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . bcb-wallet-app --overwrite --asar --platform=win32 --arch=ia32 --icon=ember-electron/resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=BCB --version-string.FileDescription=CE --version-string.ProductName=\"BCB Wallet App\"",
    "package-linux": "electron-packager . bcb-wallet-app --overwrite --asar --platform=linux --arch=x64 --icon=ember-electron/resources/icon.png --prune=true --out=release-builds"
  },
  "nodemonConfig": {
    "events": {
      "start": "yarn notify -t 'ember-electron' -m 'Starting Electron app...' -i ember-electron/icons/notification.png &",
      "restart": "yarn notify -t 'ember-electron' -m 'Restarting Electron app...' -i ember-electron/icons/notification.png &",
      "crash": "yarn notify -t 'ember-electron' -m 'Electron app has crashed!' -s Funk -i ember-electron/icons/notification.png &"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
